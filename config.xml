        </config-file>
        <config-file parent="NSBluetoothAlwaysUsageDescription" target="*-Info.plist">
            <string>Application requires bluetooth access to share files with other devices around you</string>
        </config-file>
        <config-file parent="NSMicrophoneUsageDescription" target="*-Info.plist">
            <string>Need microphone access for search city by voice</string>
        </config-file>
