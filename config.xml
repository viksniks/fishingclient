            <array>
                <string>fb</string>
                <string>whatsapp</string>
                <string>twitter</string>
            </array>
        </config-file>
        <config-file parent="GADIsAdManagerApp" target="*-Info.plist">
            <true />
        </config-file>
        <config-file parent="com.apple.developer.associated-domains" target="*-Debug.plist">
            <array>
                <string>applinks:example.org</string>
            </array>
        </config-file>
        <config-file parent="com.apple.developer.associated-domains" target="*-Release.plist">
            <array>
                <string>applinks:example.org</string>
            </array>
        </config-file>
        <config-file parent="NSLocationAlwaysUsageDescription" target="*-Info.plist">
            <string>Application requires location access to add addtional information and display to a User</string>
        </config-file>
        <config-file parent="NSLocationWhenInUseUsageDescription" target="*-Info.plist">
            <string>Application requires location access to add addtional information and display to a User</string>
        </config-file>
